{
    "name": "xamani/nova-toggle",
    "description": "A Laravel Nova toggle field.",
    "keywords": [
        "laravel",
        "nova",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Xamani\\NovaToggle\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xamani\\NovaToggle\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
